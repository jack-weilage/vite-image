{
    "name": "vite-image",
    "version": "0.7.2",
    "description": "A vitejs plugin to streamline image processing.",
    "files": [
        "dist"
    ],
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "require": "./dist/index.cjs",
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "scripts": {
        "test": "vitest --run --no-threads",
        "test:gen": "vitest --run --no-threads --update",
        "prepare": "husky install",
        "build": "tsup --dts --clean",
        "build:code": "tsup",
        "build:types": "tsup --dts-only",
        "lint": "eslint . --format friendly --report-unused-disable-directives --cache",
        "release": "npm run build && changeset publish"
    },
    "keywords": [
        "vite",
        "image",
        "imagetools",
        "typescript",
        "vitest"
    ],
    "author": "Jack Weilage",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jack-weilage/vite-image.git"
    },
    "bugs": {
        "url": "https://github.com/jack-weilage/vite-image/issues"
    },
    "packageManager": "pnpm@7.18.1",
    "engines": {
        "pnpm": "^7.0.0"
    },
    "devDependencies": {
        "@changesets/cli": "^2.25.2",
        "@types/node": "^18.11.13",
        "@types/sharp": "^0.31.0",
        "@typescript-eslint/eslint-plugin": "^5.46.0",
        "@typescript-eslint/parser": "^5.46.0",
        "@vitest/coverage-c8": "^0.25.7",
        "concurrently": "^7.6.0",
        "esbuild": "^0.16.4",
        "eslint": "^8.29.0",
        "eslint-formatter-friendly": "^7.0.0",
        "happy-dom": "^8.1.0",
        "husky": "^8.0.2",
        "lint-staged": "^13.1.0",
        "rollup": "^3.7.3",
        "tsup": "^6.5.0",
        "typescript": "^4.9.4",
        "vite": "^4.0.0",
        "vitest": "^0.25.7"
    },
    "dependencies": {
        "@rollup/pluginutils": "^5.0.2",
        "magic-string": "^0.27.0",
        "sharp": "^0.31.2",
        "validate": "^5.2.0"
    }
}