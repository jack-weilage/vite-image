{
    "name": "vite-image",
    "version": "0.7.0",
    "description": "A vitejs plugin to streamline image processing.",
    "module": "dist/index.mjs",
    "main": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "scripts": {
        "test": "vitest --run --no-threads",
        "test:gen": "vitest --run --no-threads --update",
        "prepare": "husky install",
        "build": "rollup -c rollup.config.ts --configPlugin rollup-plugin-esbuild",
        "build:code": "BUILD_TYPE=code npm run build",
        "build:types": "BUILD_TYPE=types npm run build",
        "lint": "npx eslint . --format friendly",
        "release": "npm run build && changeset publish"
    },
    "keywords": [
        "vite",
        "image",
        "imagetools",
        "typescript",
        "vitest"
    ],
    "author": "Jack Weilage",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jack-weilage/vite-image.git"
    },
    "bugs": {
        "url": "https://github.com/jack-weilage/vite-image/issues"
    },
    "packageManager": "pnpm@7.17.0",
    "engines": {
        "pnpm": "^7.0.0"
    },
    "devDependencies": {
        "@changesets/cli": "^2.25.2",
        "@types/node": "^18.11.9",
        "@types/sharp": "^0.31.0",
        "@typescript-eslint/eslint-plugin": "^5.45.0",
        "@typescript-eslint/parser": "^5.45.0",
        "concurrently": "^7.6.0",
        "esbuild": "^0.15.16",
        "eslint": "^8.28.0",
        "eslint-formatter-friendly": "^7.0.0",
        "happy-dom": "^7.7.0",
        "husky": "^8.0.2",
        "lint-staged": "^13.0.4",
        "rollup": "^3.5.0",
        "rollup-plugin-dts": "^5.0.0",
        "rollup-plugin-esbuild": "^5.0.0",
        "tslib": "^2.4.1",
        "typescript": "^4.9.3",
        "vite": "^3.2.4",
        "vitest": "^0.25.3"
    },
    "dependencies": {
        "@rollup/pluginutils": "^5.0.2",
        "magic-string": "^0.26.7",
        "sharp": "^0.31.2",
        "validate": "^5.2.0"
    }
}