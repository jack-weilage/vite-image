{
    "name": "vite-image",
    "version": "0.7.3",
    "description": "A vitejs plugin to streamline image processing.",
    "files": [
        "dist"
    ],
    "type": "module",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "require": "./dist/index.cjs",
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "scripts": {
        "test": "vitest --run --no-threads",
        "test:gen": "vitest --run --no-threads --update",
        "test:cov": "vitest --run --no-threads --coverage",
        "prepare": "husky install",
        "build": "tsup --dts --clean",
        "build:code": "tsup",
        "build:types": "tsup --dts-only",
        "lint": "eslint . --report-unused-disable-directives --cache",
        "release": "npm run build && changeset publish"
    },
    "keywords": [
        "vite",
        "image",
        "imagetools",
        "typescript",
        "vitest"
    ],
    "author": "Jack Weilage <jack@weilage.dev> (https://weilage.dev/)",
    "license": "ISC",
    "repository": "github:jack-weilage/vite-image",
    "bugs": {
        "url": "https://github.com/jack-weilage/vite-image/issues"
    },
    "packageManager": "pnpm@7.18.2",
    "engines": {
        "pnpm": "^7.0.0",
        "node": ">=16.x"
    },
    "devDependencies": {
        "@changesets/cli": "^2.25.2",
        "@types/node": "^18.11.15",
        "@types/sharp": "^0.31.0",
        "@typescript-eslint/eslint-plugin": "^5.46.1",
        "@typescript-eslint/parser": "^5.46.1",
        "@vitest/coverage-c8": "^0.25.8",
        "concurrently": "^7.6.0",
        "eslint": "^8.29.0",
        "husky": "^8.0.2",
        "lint-staged": "^13.1.0",
        "tsup": "^6.5.0",
        "typescript": "^4.9.4",
        "vite": "^4.0.1",
        "vitest": "^0.25.8"
    },
    "dependencies": {
        "magic-string": "^0.27.0",
        "sharp": "^0.31.2",
        "validate": "^5.2.0"
    },
    "peerDependencies": {
        "vite": "^4.0.0"
    }
}