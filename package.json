{
    "name": "vite-image",
    "version": "0.4.0",
    "description": "A vitejs plugin to streamline image processing.",
    "module": "dist/index.mjs",
    "main": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "scripts": {
        "test": "vitest --run --no-threads",
        "build": "rimraf dist && rollup -c rollup.config.ts --configPlugin rollup-plugin-esbuild",
        "release": "pnpm build && pnpm changeset publish"
    },
    "keywords": [
        "vite",
        "image",
        "imagetools",
        "typescript",
        "vitest"
    ],
    "author": "Jack Weilage",
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jack-weilage/vite-image.git"
    },
    "bugs": {
        "url": "https://github.com/jack-weilage/vite-image/issues"
    },
    "packageManager": "pnpm@7.15.0",
    "engines": {
        "pnpm": "^7.0.0"
    },
    "devDependencies": {
        "@changesets/cli": "^2.25.2",
        "@types/node": "^18.11.9",
        "@types/sharp": "^0.31.0",
        "esbuild": "^0.15.13",
        "happy-dom": "^7.6.7",
        "rimraf": "^3.0.2",
        "rollup": "^3.2.5",
        "rollup-plugin-dts": "^5.0.0",
        "rollup-plugin-esbuild": "^5.0.0",
        "tslib": "^2.4.1",
        "typescript": "^4.8.4",
        "vite": "^3.2.3",
        "vitest": "^0.25.1"
    },
    "dependencies": {
        "@rollup/pluginutils": "^5.0.2",
        "magic-string": "^0.26.7",
        "sharp": "^0.31.2"
    }
}